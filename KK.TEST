SUBROUTINE DEMAND
WINDOWS

TARGET=ACCOUNT


DEFINE
 #INCLUDE "LIB.HTML.DEF"
 
 TRUE=1
 FALSE=0
END [DEFINE]


SETUP
 HTMLFNAME="KK.TEST.HTML"
 HTMLPAGENAME="mainPage"
 WHILE HTMLPAGENAME<>""
  DO     
   CALL CREATEPAGE
  END
END [SETUP]


SELECT 
 NONE
END [SELECT]


PRINT TITLE="KK.TEST"
 SUPPRESS 1
END [PRINT]


