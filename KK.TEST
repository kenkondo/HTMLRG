PROCEDURE REPLACEHTML
 IF (HTMLREPLACENAME="myUrl") THEN
  DO
   HTMLVIEWLINE("var myUrl='myUrl='https://admin.demo.lumindigital.com/login';")
  END
END [REPLACEHTML]


PROCEDURE SETVARIABLES
 IF HTMLINPUTNAME="HTMLPAGENAME" THEN
  HTMLPAGENAME=HTMLINPUTVALUE
END [SETVARIABLES] 

 
